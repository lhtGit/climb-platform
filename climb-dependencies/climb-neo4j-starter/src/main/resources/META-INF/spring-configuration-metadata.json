{
  "properties": [
    {
      "name": "neo4j.datasource",
      "type": "com.climb.neo4j.properties.Neo4jDataSourceProperties",
      "sourceType": "com.climb.neo4j.properties.Neo4jDataSourceProperties"
    },
    {
      "name": "neo4j.datasource.url",
      "type": "java.lang.String",
      "sourceType": "com.climb.neo4j.properties.Neo4jDataSourceProperties",
      "description": "neo4j url jdbc:neo4j:bolt://ip:port"
    },
    {
      "name": "neo4j.datasource.username",
      "type": "java.lang.String",
      "sourceType": "com.climb.neo4j.properties.Neo4jDataSourceProperties",
      "description": "neo4j username"
    },
    {
      "name": "neo4j.datasource.password",
      "type": "java.lang.String",
      "sourceType": "com.climb.neo4j.properties.Neo4jDataSourceProperties",
      "description": "neo4j passoword"
    },
    {
      "name": "neo4j.datasource.driverClassName",
      "type": "java.lang.String",
      "sourceType": "com.climb.neo4j.properties.Neo4jDataSourceProperties",
      "description": "neo4j driver",
      "defaultValue": "org.neo4j.jdbc.Driver"
    },
    {
      "name": "neo4j.datasource.database",
      "type": "java.lang.String",
      "sourceType": "com.climb.neo4j.properties.Neo4jDataSourceProperties",
      "description": "neo4j database"
    },
    {
      "name": "neo4j.datasource.initialSize",
      "type": "java.lang.Integer",
      "sourceType": "com.climb.neo4j.properties.Neo4jDataSourceProperties",
      "description": "初始化大小",
      "defaultValue": "5"
    },
    {
      "name": "neo4j.datasource.minIdle",
      "type": "java.lang.Integer",
      "sourceType": "com.climb.neo4j.properties.Neo4jDataSourceProperties",
      "description": "最小",
      "defaultValue": "5"
    },
    {
      "name": "neo4j.datasource.maxActive",
      "type": "java.lang.Integer",
      "sourceType": "com.climb.neo4j.properties.Neo4jDataSourceProperties",
      "description": "最大",
      "defaultValue": "20"
    },
    {
      "name": "neo4j.datasource.maxWait",
      "type": "java.lang.Long",
      "sourceType": "com.climb.neo4j.properties.Neo4jDataSourceProperties",
      "description": "配置获取连接等待超时的时间",
      "defaultValue": "60000"
    },
    {
      "name": "neo4j.datasource.timeBetweenEvictionRunsMillis",
      "type": "java.lang.Long",
      "sourceType": "com.climb.neo4j.properties.Neo4jDataSourceProperties",
      "description": "配置间隔多久才进行一次检测，检测需要关闭的空闲连接，单位是毫秒",
      "defaultValue": "60000"
    },
    {
      "name": "neo4j.datasource.minEvictableIdleTimeMillis",
      "type": "java.lang.Long",
      "sourceType": "com.climb.neo4j.properties.Neo4jDataSourceProperties",
      "description": "置一个连接在池中最小生存的时间，单位是毫秒",
      "defaultValue": "300000"
    },
    {
      "name": "neo4j.datasource.validationQuery",
      "type": "java.lang.String",
      "sourceType": "com.climb.neo4j.properties.Neo4jDataSourceProperties",
      "description": "校验SQL，Oracle配置 spring.datasource.validationQuery= SELECT 1 FROM DUAL，如果不配validationQuery项，则下面三项配置无用"
    },
    {
      "name": "neo4j.datasource.testWhileIdle",
      "type": "java.lang.Boolean",
      "sourceType": "com.climb.neo4j.properties.Neo4jDataSourceProperties",
      "defaultValue": "false"
    },
    {
      "name": "neo4j.datasource.testOnBorrow",
      "type": "java.lang.Boolean",
      "sourceType": "com.climb.neo4j.properties.Neo4jDataSourceProperties",
      "defaultValue": "false"
    },
    {
      "name": "neo4j.datasource.testOnReturn",
      "type": "java.lang.Boolean",
      "sourceType": "com.climb.neo4j.properties.Neo4jDataSourceProperties",
      "defaultValue": "false"
    },
    {
      "name": "neo4j.datasource.poolPreparedStatements",
      "type": "java.lang.Boolean",
      "sourceType": "com.climb.neo4j.properties.Neo4jDataSourceProperties",
      "description": "打开PSCache，并且指定每个连接上PSCache的大小，一般用于存储过程 例如 oracle，会提升效率",
      "defaultValue": "false"
    },
    {
      "name": "neo4j.datasource.maxPoolPreparedStatementPerConnectionSize",
      "type": "java.lang.Integer",
      "sourceType": "com.climb.neo4j.properties.Neo4jDataSourceProperties",
      "defaultValue": "20"
    },
    {
      "name": "neo4j.datasource.poolPreparedStatements",
      "type": "java.lang.Boolean",
      "sourceType": "com.climb.neo4j.properties.Neo4jDataSourceProperties",
      "description": "合并多个DruidDataSource的监控数据",
      "defaultValue": "true"
    },
    {
      "name": "neo4j.datasource.poolPreparedStatements",
      "type": "java.lang.String",
      "sourceType": "com.climb.neo4j.properties.Neo4jDataSourceProperties",
      "description": "配置监控统计拦截的filters，去掉后监控界面sql无法统计，'wall'用于防火墙",
      "defaultValue": "stat"
    },
    {
      "name": "neo4j.datasource.poolPreparedStatements",
      "type": "java.util.Properties",
      "sourceType": "com.climb.neo4j.properties.Neo4jDataSourceProperties",
      "description": "其他配置",
      "defaultValue": "Properties"
    },
    {
      "name": "neo4j.mybatis",
      "type": "com.climb.neo4j.config.Neo4jMybatisMappingConfig",
      "sourceType": "com.climb.neo4j.config.Neo4jMybatisMappingConfig"
    },
    {
      "name": "neo4j.mybatis.basePackages",
      "type": "java.lang.String[]",
      "sourceType": "com.climb.neo4j.config.Neo4jMybatisMappingConfig",
      "description": "mybatis 扫描dao位置"
    },
    {
      "name": "neo4j.mybatis.localtionPattern",
      "type": "java.lang.String",
      "sourceType": "com.climb.neo4j.config.Neo4jMybatisMappingConfig",
      "description": "mybatis 扫描Resource xml位置"
    },
    {
      "name": "neo4j.enable-open-local-transaction",
      "type": "java.lang.Boolean",
      "sourceType": "com.climb.neo4j.config.ChainedTransactionManagerConfig",
      "description": "开启neo4j本地事务，并注册链式事务管理器，该管理器会将会有事务管理器全部注册进去",
      "defaultValue": false
    }
  ]
}